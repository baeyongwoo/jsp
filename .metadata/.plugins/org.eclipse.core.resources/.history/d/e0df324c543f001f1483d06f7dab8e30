package Ex0711;

import java.util.HashMap;
import java.util.Map;

public class Login {

	private String id;
	private String pw;
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getPw() {
		return pw;
	}
	public void setPw(String pw) {
		this.pw = pw;
	}
	
	
	public Map<String, String> login(String id, String pw){
		
		System.out.println("form에서 전달 받은 값 : " + id + pw);
		Map<String, String> userInfo = new HashMap<>(){{
			put("id", id);
			put("pw", pw);
		}};
		
		
		return userInfo;
	}
}
